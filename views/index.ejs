<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="UTF-8" />
    <title>User list</title>
  </head>
  <body>
    <table>
      <tr>
        <th>名前</th>
        <th>email</th>
        <th>更新</th>
        <th>削除</th>
      </tr>
      <% users.forEach(function (value) { %>
      <tr>
        <td><%= value.name %></td>
        <td><%= value.email %></td>
        <td><a href="/edit/<%= value.id %>">更新</a></td>
        <td><a href="/delete/<%= value.id %>">削除</a></td>
      </tr>
      <% }); %>
    </table>
  </body>
</html>
